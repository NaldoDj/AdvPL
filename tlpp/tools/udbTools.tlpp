    lRet:=self:ToJSON(@cQuery,@cFile,@cJSONMain,@lX3Titulo,@lIEmptyFields,@lHeader,@lRowNumber)

    if (lRet)
        cMsg:="Arquivo gerado com sucesso :: "+cFile
